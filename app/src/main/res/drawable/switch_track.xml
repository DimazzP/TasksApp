<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Saat Switch aktif -->
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/switch_track_enabled" /> <!-- Warna track saat aktif -->
            <corners android:radius="12dp" />
        </shape>
    </item>
    <!-- Saat Switch tidak aktif -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/switch_track_disabled" /> <!-- Warna track saat tidak aktif -->
            <corners android:radius="12dp" />
        </shape>
    </item>
</selector>
